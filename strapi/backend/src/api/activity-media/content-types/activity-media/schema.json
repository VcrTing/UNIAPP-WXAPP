{
  "kind": "collectionType",
  "collectionName": "activity_medias",
  "info": {
    "singularName": "activity-media",
    "pluralName": "activity-medias",
    "displayName": "ActivityMedia"
  },
  "options": {
    "draftAndPublish": true
  },
  "pluginOptions": {},
  "attributes": {
    "mediaType": {
      "type": "integer",
      "default": 0
    },
    "url": {
      "type": "text"
    },
    "sortOrder": {
      "type": "integer",
      "default": 1
    },
    "isSex": {
      "type": "integer",
      "default": 0
    },
    "isGallery": {
      "type": "integer",
      "default": 0
    },
    "activity": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::activity.activity",
      "inversedBy": "activity_medias"
    },
    "urlSmall": {
      "type": "text"
    },
    "user": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "activity_medias"
    },
    "media": {
      "type": "media",
      "multiple": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "w": {
      "type": "decimal"
    },
    "h": {
      "type": "decimal"
    },
    "isAllow": {
      "type": "integer",
      "default": 1
    },
    "product": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::product.product",
      "inversedBy": "medias"
    }
  }
}
