    "title": {
      "type": "string"
    },
    "startTime": {
      "type": "datetime"
    },
    "endTime": {
      "type": "datetime"
    },
    "endJoinTime": {
      "type": "datetime"
    },
    "participantLimit": {
      "type": "integer"
    },
    "fee": {
      "type": "decimal"
    },
    "introduction": {
      "type": "text"
    },
    "reviewStatus": {
      "type": "integer"
    },
    "reviewComment": {
      "type": "text"
    },
    "shareCount": {
      "type": "integer"
    },
    "viewCount": {
      "type": "integer"
    },
    "isRecommended": {
      "type": "integer"
    },
    "typed": {
      "type": "integer",
      "default": 1
    },
    "activity_medias": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity-media.activity-media",
      "mappedBy": "activity"
    },
    "activity_tags": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::activity-tag.activity-tag",
      "inversedBy": "activities"
    },
    "publisher": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "plugin::users-permissions.user",
      "inversedBy": "activities"
    },
    "dataStatus": {
      "type": "integer"
    },
    "activity_registrations": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity-registration.activity-registration",
      "mappedBy": "activity"
    },
    "longitude": {
      "type": "decimal"
    },
    "latitude": {
      "type": "decimal"
    },
    "address": {
      "type": "string"
    },
    "city": {
      "type": "string"
    },
    "area": {
      "type": "string"
    },
    "activity_invites": {
      "type": "relation",
      "relation": "oneToMany",
      "target": "api::activity-invite.activity-invite",
      "mappedBy": "activity"
    },
    "activity_address": {
      "type": "relation",
      "relation": "manyToOne",
      "target": "api::activity-address.activity-address",
      "inversedBy": "activities"
    }
  }
